<?xml version="1.0" encoding="utf-8"?>
<com.mine.class_schedule.ui.classview.ClassView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:showIn="@layout/classtable_row"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:background="@drawable/ripple_background">
    <TextView
        android:id="@+id/text_classname"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/text_classplace"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/text_classplace"
        app:layout_constraintTop_toBottomOf="@+id/text_classname"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</com.mine.class_schedule.ui.classview.ClassView>
